{
  "name": "test-pilot",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:desktop": "cross-env VITE_APP_ENV=desktop vite dev",
    "build": "npm run build:web",
    "build:web": "cross-env VITE_APP_ENV=web vite build",
    "build:desktop": "cross-env VITE_APP_ENV=desktop vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:push": "npx drizzle-kit push",
    "drizzle:studio": "npx drizzle-kit studio",
    "db:seed": "npx tsx seed.ts",
    "db:test": "node test-db.js",
    "setup": "npm run db:test && npm run drizzle:push && npm run db:seed",
    "dev:with-db": "npm run setup && npm run dev",
    "test": "npm run test:unit -- --run",
    "tauri": "tauri",
    "tauri:dev": "cross-env VITE_APP_ENV=desktop tauri dev",
    "tauri:build": "npm run build:desktop && tauri build --debug",
    "deploy": "npm run build:web && vercel deploy"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/cli": "^2.6.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-parser": "^4.0.3",
    "@vitest/browser": "^3.2.3",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "playwright": "^1.53.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "@tauri-apps/plugin-http": "^2.5.0",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.44.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.10.1",
    "openapi-types": "^12.1.3",
    "postgres": "^3.4.7",
    "swagger-parser": "^10.0.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zod-from-json-schema": "^0.4.2"
  }
}
